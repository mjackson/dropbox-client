{
  "name": "dropbox-client",
  "version": "3.0.3",
  "description": "Dropbox API client",
  "author": "Michael Jackson",
  "license": "MIT",
  "main": "lib",
  "files": [
    "lib",
    "umd"
  ],
  "scripts": {
    "lint": "eslint modules",
    "test": "npm run lint && mocha -r babel-register -r modules/__tests__/globals modules/**/*-test.js",
    "build": "node ./scripts/build.js",
    "build-cjs": "rimraf lib && babel ./modules -d lib --ignore '__tests__'",
    "build-min": "webpack -p modules/index.js umd/dropbox-client.min.js",
    "build-umd": "webpack modules/index.js umd/dropbox-client.js",
    "release": "node ./scripts/release.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "http-client": "^4.0.0",
    "query-string": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "expect": "^1.18.0",
    "gzip-size": "^3.0.0",
    "mocha": "^3.0.2",
    "pretty-bytes": "^4.0.2",
    "readline-sync": "^1.4.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjackson/dropbox-client.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ]
  }
}
